<h1>Super cars! (guitar solo)</h1>

    <div class="car" style="font-size:5em">
      <p class="details">
        <strong>Make:</strong> <%= @car.make.make %> |
        <strong>Model:</strong> <%= @car.model.model %> |
        <strong>Trim:</strong> <%= @car.trim.trim %> |
        <strong>Style:</strong> <%= @car.style.body_style %> |
        <strong>Year:</strong> <%= @car.year %>
        <strong>Color:</strong> <%= @car.color %>
      </p>
    </div>
